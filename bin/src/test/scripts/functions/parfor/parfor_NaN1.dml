nr = $1;
xr = $2;
NaNval = 0/0;

R = matrix(0, rows=nr, cols=nr); 
R[1:xr,] = matrix(NaNval, rows=xr, cols=nr);

parfor( i in 1:nr )
{
   R[i,i] = i^2 + 7;           
}   

write(R, $3);       