numIter = $1;

s = "# Name Value";
for( i in 1:numIter )
{
   # more than 100 bytes (will throw an error for more than 10000 iterations)
   s = append(s, "A["+i+"] = ABCDEFGHIJKLMNOPQRSTUVWXYZ ABCDEFGHIJKLMNOPQRSTUVWXYZ ABCDEFGHIJKLMNOPQRSTUVWXYZ ABCDEFGHIJKLMNOPQRSTUVWXYZ, " +i);
}

write(s, $2);
